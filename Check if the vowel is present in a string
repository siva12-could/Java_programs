// Check if the vowel is present in a string
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner input = new Scanner(System.in);
	    String str = input.next(); // Take the string has input;
	    int c = input.nextInt(); // Initialize the c is equal to zero;
	   for(int i=0;i<str.length();i++){
	       if(str.charAt(i)=='a' || str.charAt(i)=='e' || str.charAt(i)=='i' || str.charAt(i)=='o' || str.charAt(i)=='u'){
	           System.out.println(true); // If the vowel is present it will print true
	           c=c+1;
	           break;
	       }
	       
	   }
	   if(c==0){
	       System.out.println(false); // If no vowels are present in the string it will print false;
	   }
	   
	    

	}
}
